CLASSES AND OBJECTS IN JAVA
public class myclass {
    public static void main(String[] args) {

        student mark = new student();
        mark.id = 1;
        mark.name = "mark";
        mark.age = 15;

        student tom = new student();
        tom.id = 2;
        tom.name = "toms";
        tom.age = 15;

        System.out.println(mark.id + " is " + mark.age + " year old");
        System.out.println(tom.id + " is " + tom.age + " year old");
    }
}
